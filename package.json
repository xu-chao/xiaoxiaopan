{
  "name": "xiaoxiaopan",
  "title": "小小潘",
  "version": "2.5.0",
  "description": "小小潘 Electron Based",
  "buildinf": "Stable Build 2023-03-20",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [
    "AI",
    "GirlFriend"
  ],
  "author": "xuchao",
  "license": "GPL",
  "build": {
    "productName": "小小潘",
    "appId": "com.xuchao.xiaoxiaopan",
    "copyright": "2023 xiaoxiaopan Studio All Rights Reserved.",
    "asar": true,
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "win": {
      "target": [
        {
          "target": "nsis"
        }
      ],
      "icon": "./assets/app.ico",
      "asarUnpack": [
        "src/**",
        "README.md"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./install.ico",
      "uninstallerIcon": "./install.ico",
      "installerHeaderIcon": "./assets/app.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "小小潘"
    }
  },
  "devDependencies": {
    "electron": "^22.0.0"
  },
  "dependencies": {
    "axios": "^1.3.4"
  }
}
